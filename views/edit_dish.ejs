
    <h1>edit dish</h1>
    <h2><%= dish.id %></h2>

    <form action="/dishes/<%= dish.id %>/edit" method="post">
        <input type="hidden" name="_method" value ="put">
        <label for="">title</label>
        <input type="text" name="title" value="<%= dish.title %>"/>
        <label for="">imagem url</label>
        <input type="text" name="imagem_url" value="<%= dish.imagem_url %>"/>
        <button>share</button>
    </form>
